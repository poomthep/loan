{
  "version": 2,
  "public": true,
  "routes": [
    {
      "src": "/sw.js",
      "headers": {
        "cache-control": "no-store"
      }
    },
    {
      "src": "/public/(.*)",
      "headers": {
        "cache-control": "no-store"
      }
    },
    {
      "src": "/(index\\.html|admin\\.html)$",
      "headers": {
        "cache-control": "no-store"
      }
    },
    {
      "src": "/api/config",
      "dest": "/api/config.js"
    }
  ],
  "headers": [
    {
      "source": "/(index.html|admin.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store"
        }
      ]
    }
  ]
}