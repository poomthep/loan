{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    { "src": "/loan", "dest": "/loan.html" },
    { "src": "/admin", "dest": "/admin.html" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "framework": "vite",
  "outputDirectory": "dist"
}
